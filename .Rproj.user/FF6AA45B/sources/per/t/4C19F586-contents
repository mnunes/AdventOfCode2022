planilha_1 <- tibble (aluno = c("Cassia", "Kelly", "Joana"),
                      nota = c(4, 10, 8),
                      planilha = 1)


planilha_2 <- tibble(aluno = c("Rebeca", "Carlos", "Viviane"),
                     conceito = c(4, 7, 85),
                     planilha = 2)


df <- bind_rows(planilha_1, planilha_2)

planilha_1 |> 
  full_join(planilha_2)

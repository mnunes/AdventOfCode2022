rucksack <- function(x){
n <- nchar(x)
h1 <- substr(x, 1, n/2)
h2 <- substr(x, n/2+1, n)
char_h1 <- unlist(strsplit(h1, ""))
char_h2 <- unlist(strsplit(h2, ""))
for (i in char_h1){
for (j in char_h2){
if (i == j){
item <- i
}
}
}
return(item)
}
